{
  "name": "remark-typedoc-astro-link",
  "private": true,
  "version": "0.0.0",
  "description": "Remark plugin to transform typedoc links to astro compatible links",
  "author": "rai",
  "keywords": ["remark", "remark-plugin"],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "src",
    "dist",
    "!**/*.test.ts",
    "!**/*.spec.ts",
    "!**/*.tsbuildinfo",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "test": "vitest",
    "test:ci": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:coverage:ci": "vitest run --coverage",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "prepublishOnly": "bun run build",
    "check": "biome check .",
    "check:write": "biome check --write .",
    "release": "bun run vitest run && bun run build && bun run changeset publish"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/hast": "3.0.4",
    "@types/jsdom": "27.0.0",
    "@types/mdast": "4.0.4",
    "dedent": "1.7.0",
    "jsdom": "27.2.0",
    "rehype-stringify": "10.0.1",
    "remark-parse": "11.0.0",
    "remark-rehype": "11.1.2",
    "unified": "11.0.5",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "defu": "^6.1.4",
    "unist-util-visit": "^5.0.0"
  }
}
